{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["alloy", "alloy/react", "alloy/typescript"],
	"parser": "@typescript-eslint/parser",
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "@typescript-eslint/eslint-plugin"],
	"rules": {}
}
